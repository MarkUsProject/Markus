<script type="text/javascript">


  function load_test_result(test_result_id) {
    jQuery('#loading_test_result').show();
    jQuery('#select_test_result_id').disable();
    window.open('<%= render_test_result_assignment_path(:id => @assignment.id, :aid => aid)%>&test_result_id='+test_result_id,
    "Test Results", "width=400,height=250,resizable=yes,scrollbars=yes")
    jQuery('#loading_test_result').hide();
    jQuery('#select_test_result_id').enable();
  }

</script>
