<div id="create-template-parsing-modal-content" hidden>
  <%= f.label :automatic_parsing, t('exam_templates.parsing.general') %>
  <%= f.check_box :automatic_parsing, value: false %>
  <section class='dialog-actions'>
    <input id="create-template-parsing-previous" class='create-template-view-change' type='button' value='Previous'/>
    <%= f.submit t(:save),
                 name: 'submit',
                 data: { disable_with: t('working') },
                 class: 'button' %>
    <input type='reset' value='<%= t(:cancel) %>'/>
  </section>
</div>
