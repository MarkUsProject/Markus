<% content_for :head do %>
  <%= render partial: 'template_divisions_table',
             formats: [:'js.jsx'],
             handlers: [:erb] %>
  <script>
    <%= render partial: 'boot',
               formats: [:js] %>
  </script>
<% end %>

<% content_for :title, ExamTemplate.model_name.human.pluralize %>

<% @heading_buttons = [
    { link_text: t('exam_templates.create.add_new'),
      link_path: '#',
      html_options: { onclick:'modal_create_new.open()' } }
] %>

<!-- Modals -->
<%= render partial: 'create_new_template',
           layout: 'layouts/modal_dialog'  %>

<%= render partial: 'form',
           locals: { exam_template: @exam_templates[0] }%>

<% content_for :wrap_left_content do %>
  <!--The table that contains all the divisions.-->
  <div id='divisions_table'></div>
<% end %>
