<table class='extra_marks_pane_list'>
  <thead>
    <tr>
      <th><%= ExtraMark.human_attribute_name(:description) %></th>
      <th><%= ExtraMark.human_attribute_name(:extra_mark).pluralize %></th>
    </tr>
  </thead>
  <tbody id='extra_marks_list'>
    <% extra_marks_points.each do |extra_mark| %>
      <%= render partial: 'results/student/extra_mark',
                 locals: { extra_mark: extra_mark,
                           result_id: result_id } %>
    <% end %>
  </tbody>
</table>
