<% if session[:job_id] %>
  <% job_status = ActiveJob::Status.get(session[:job_id]) %>
  <% if job_status[:job_class] %>
    poll_job('<%= session[:job_id] %>', undefined, <%= raw job_status[:job_class].on_complete_js(job_status) %>);
  <% else %>
    <% session[:job_id] = nil %>
  <% end %>
<% end %>
